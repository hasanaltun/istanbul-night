{
	"name": "Istanbul Night",
	"author": "Hasan Altun",
    "type": "dark",
	"colors": {
		"editor.background": "#181a20",
		"editor.foreground": "#D5D5D5",
		"activityBarBadge.background": "#007ad0",
		"sideBarTitle.foreground": "#646875",
		"sideBar.foreground": "#dadde6",
		"sideBar.background": "#1f2227",

		"sideBarSectionHeader.background": "#1f2227",
		"sideBarSectionHeader.foreground": "#dadde6",
		"sideBarSectionHeader.border": "#30333d",

		"titleBar.activeForeground": "#fefefe",
		"titleBar.inactiveForeground": "#d5d5d5",
		"titleBar.activeBackground": "#1f2227",
		"titleBar.inactiveBackground": "#1f2227",
		"titleBar.border": "#30333d",

		"activityBar.background": "#1f2227",
		"activityBar.border": "#30333d",

		"statusBar.foreground": "#969aa7",
		"statusBar.background": "#3c3e4a",
		"statusBar.border": "#3c3e4a",

		"tab.activeBackground": "#2b2d36",
		"tab.activeForeground": "#dadde6",

		"breadcrumb.foreground": "#525770",
		"breadcrumb.activeSelectionForeground": "#aab1d6",
		
		"editorGroup.dropBackground": "#181a20",
		"editorGroupHeader.tabsBackground": "#181a20"
	},
	"tokenColors": [
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown",
				"variable.legacy.builtin.python"
			],
			"settings": {
				"foreground": "#6abc8b"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#569cd7"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#747780"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#7cadfa"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#4ec7d7"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#6abc8b"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#dd9b75"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#c1d48a"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#c1d48a"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#c1d48a"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#cc4b54"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d5d8e1"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7cadfa"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#66cdaa"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#f2868d"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#7cadfa"
			}
		},
		{
			"scope": "punctuation.definition.quote.begin.markdown",
			"settings": {
				"foreground": "#747780"
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#dd9b75"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#6abc8b"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#d5d8e1"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#7cadfa"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#6abc8b"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#7cadfa"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#6abc8b"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#d5d8e1"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#9e89ec"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#9e89ec"
			}
		},
		{
			"scope": [
				"storage.modifier",
				"keyword.operator.noexcept"
			],
			"settings": {
				"foreground": "#9e89ec"
			}
		},
		{
			"scope": [
				"string",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#6abc8b"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#6abc8b"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#6abc8b"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#6abc8b"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#dd9b75"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#9e89ec"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#d5d8e1"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#9e89ec"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#9e89ec"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#d5d8e1"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#9e89ec"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#4ec7d7"
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#7cadfa"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#9cdcff"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#b6ceb8"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#d5d8e1"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#c2d58b"
			}
		},
		{
			"scope": "log.error",
			"settings": {
				"foreground": "#cd4c54"
			}
		},
		{
			"scope": "log.warning",
			"settings": {
				"foreground": "#ec9b62"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"newOperator": "#d5d8e1",
		"stringLiteral": "#6abc8b",
		"customLiteral": "#d5d8e1",
		"numberLiteral": "#4ec7d7"
	}
}